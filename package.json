{
  "name": "create-tags",
  "version": "0.0.0",
  "description": "Create multiple tags",
  "main": "/src/index.ts",
  "scripts": {
    "prepare": "husky",
    "start": "npm install && npm run build",
    "build": "esbuild ./src --outfile=dist/index.js --bundle --platform=node",
    "test": "jest .",
    "test:watch": "jest . --watch",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "check-types": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "all": "npm run check-types && npm run lint:fix -- --max-warnings=0 && npm run format && npm run build"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "Brady Clifford",
  "license": "ISC",
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{js,yml,json,ts,md}": [
      "prettier --write"
    ],
    "*.{yml,yaml}": [
      "yamllint"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.10",
    "@actions/github": "^6.0.0",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23",
    "@babel/preset-env": "^7.23",
    "@babel/preset-typescript": "^7.23",
    "@octokit/types": "^12.6.0",
    "@tsconfig/recommended": "^1.0.3",
    "@types/jest": "^29.5.12",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^7.1",
    "@typescript-eslint/parser": "^7.1",
    "babel-jest": "^29.7",
    "esbuild": "^0.20",
    "eslint": "^8.57",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7",
    "lint-staged": "^15.2",
    "prettier": "^3.2",
    "typescript": "^5.5.4",
    "yaml-lint": "^1.7"
  }
}
