{
  "name": "create-tags",
  "version": "0.0.0",
  "description": "Create multiple tags",
  "main": "/src/index.ts",
  "scripts": {
    "prepare": "husky install",
    "start": "npm install && npm run build",
    "build": "esbuild ./src --outfile=dist/index.js --bundle --platform=node --sourcemap",
    "test": "jest .",
    "test:watch": "jest . --watch",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "check-types": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "all": "npm run check-types && npm run lint:fix -- --max-warnings=0 && npm run format && npm run build"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "Brady Clifford",
  "license": "ISC",
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{js,yml,json,ts,md}": [
      "prettier --write"
    ],
    "*.{yml,yaml}": [
      "yamllint"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.10",
    "@actions/github": "^5.1.1",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.21",
    "@babel/preset-env": "^7.20",
    "@babel/preset-typescript": "^7.21",
    "@octokit/types": "^9.0.0",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.0",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.55",
    "@typescript-eslint/parser": "^5.55",
    "babel-jest": "^29.5",
    "esbuild": "^0.17",
    "eslint": "^8.37",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5",
    "lint-staged": "^13.2",
    "prettier": "^2.8",
    "typescript": "^5.0.2",
    "yaml-lint": "^1.7"
  }
}
